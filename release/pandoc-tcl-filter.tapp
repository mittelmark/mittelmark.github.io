package require Tcl

package provide mthinx 0.1


namespace eval mthinx {
    namespace eval ::mthinx::mthinx { }
    proc usage { } {
        puts "mthinx.tcl \[OPTIONS\] \[FILES\]"
        
    }
    proc thingy name {
        proc ::$name args "namespace eval ::$name \$args"
    } 
    
    proc mthinx {name args} {
        global thingies_
        set name_ "::mthinx[incr thingies_]"
        thingy $name_
        interp alias {} $name {} $name_
        namespace eval $name_ "namespace path ::mthinx"
        foreach {key val} $args {
            set k [string trimleft ${key} -] 
            namespace eval $name_ "set $k $val"
        }
    }
    proc destroy {} {
        set self [uplevel 1 namespace current]
        namespace delete $self
        rename $self ""
    }
    proc instvar args { 
        foreach var $args { uplevel 1 variable $var } 
    } 
    proc my {args} {
        uplevel 1 {*}$args
    }
    proc myvar varname { 
        set nsp [uplevel 1 namespace current]
        return ${nsp}::$varname 
    } 
    proc self { } {
        set self [uplevel 1 namespace current]
    }
}
if {[info exists argv0] && $argv0 eq [info script] && [regexp ... $argv0]} {
    if {[llength $argv] > 0} {
        if {[lindex $argv 0] eq "--help"} {
            mthinx::usage
        } elseif  {[lindex $argv 0] eq "--version"} {
            puts "[package present mthinx]"
        } elseif {[lindex $argv 0] eq "--test"} {
            package require tcltest
            set argv [list] 
            tcltest::test dummy-1.1 {
                Calling my proc should always return a list of at least length 3
            } -body {
                set result 1
            } -result {1}
            tcltest::cleanupTests
            catch { destroy . }
        }
    } else {
        mthinx::usage
    }   
}


mthinx::mthinx t1 -x 5 -y 6
puts [t1 self]
t1 proc hello {} {
    puts Hello!
}
t1 hello
puts [t1 set x]

t1 proc getX1 {} {
    instvar x y
    return "x=$x y=$y"
}

puts [t1 getX1]

t1 proc getX2 {} {
    set self [self]
    $self set x
}

puts [t1 getX2]

t1 proc getX3 {} {
    my variable x
    set x
}

puts "x3: [t1 getX3]"
